{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "localStorage": false,
    "window": false,
    "Audio": false,
    "io": false,
    "$": false,
    "document": false
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "linebreak-style": "off"
  }
}
